  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "@aave/math-utils": "^1.29.1",
    "@aave/protocol-js": "^4.3.0",
    "@ant-design/icons": "^5.4.0",
    "@apollo/client": "latest",
    "@apollo/experimental-nextjs-app-support": "^0.11.2",
    "@ethersproject/units": "^5.7.0",
    "@graphprotocol/graph-cli": "^0.79.0",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "antd": "^5.19.3",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.12",
    "dotenv": "^16.4.5",
    "eth-hooks": "^4.6.2",
    "ethers": "^5.7.2",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.0",
    "ramda": "^0.30.1",
    "web3": "^4.11.1"
